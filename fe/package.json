    "@heroicons/vue": "^2.1.1",
    "axios": "^1.6.7",
    "brackets-json-db": "^1.0.2",
    "brackets-manager": "^1.6.4",
    "flowbite": "^2.3.0",
    "flowbite-vue": "^0.1.3",
